#include "Brick.h"
#include <allegro5/allegro_primitives.h>

void Square::draw() {
	al_draw_rectangle(origin.X(), origin.Y(), origin.X()+length, origin.Y()+height, al_map_rgb(150, 150, 255), 1);
}

double Square::bBoxMaxX() {
   return length;
}

double Square::bBoxMaxY() {
   return height;
}

double Square::bBoxMinX() {
   return origin.X();
}


double Square::bBoxMinY() {
   return origin.Y();
}
